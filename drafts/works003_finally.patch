+  } catch (e) {
+    dump("Outer error: " + e + "\n");
+    payload.requestPostData.postData = { text: null };
+    payload.requestPostData.uploadHeaders = { headers: [], headersSize: 0 };
+  }
+
+  dump("Returning payload: " + JSON.stringify(payload) + "\n");
+  return payload;
+}
